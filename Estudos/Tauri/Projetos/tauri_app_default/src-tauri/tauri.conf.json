{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri_app_default",
  "version": "0.1.0",
  "identifier": "com.lalunainsky.tauri_app_default",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "tauri_app_default",
        "maximized": true,
        "resizable": false,
        "fullscreen": false,
        "backgroundColor": "#000000"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
